    "prettier": "prettier --write \"**/*.@(js|md|yml|json)\"",
    "prettier:check": "prettier --check \"**/*.@(js|md|yml|json)\"",
    "pretest": "npm run lint",
    "test": "npm run -s unit",
    "posttest": "npm run -s prettier:check"
