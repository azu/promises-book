#!/usr/bin/env node
"use strict";

var parse = require("../lib/parse"),
    stdin = process.stdin,
    stdout = process.stdout,
    inputChunks = [];

stdin.resume();
stdin.setEncoding('utf8');

stdin.on('data', function (chunk) { inputChunks.push(chunk); });

stdin.on('end', function () {
  var input = inputChunks.join(),
      parsedData = parse(inputJSON),
      outputJSON = JSON.stringify(parsedData);
  stdout.write(outputJSON);
  stdout.write('\n');
});
