    "babel-register": "^6.24.1",
    "cross-env": "^5.2.0",
    "mocha": "^6.1.4",
    "power-assert": "^1.6.1",
    "rimraf": "^2.6.3",
    "textlint-rule-no-todo": "^2.0.1"
